{
  "name": "taxonpages",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:ssr": "node server",
    "dev": "vite dev",
    "build": "vite build",
    "build:ssr": "npm run build:client && npm run build:server",
