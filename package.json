    "js-yaml": "^4.1.0",
    "leaflet": "^1.9.4",
    "leaflet.markercluster": "^1.5.3",
    "markdown-it-anchor": "^9.0.1",
    "pinia": "^2.2.2",
    "unhead": "^1.9.16",
    "unplugin-vue-markdown": "^0.26.2",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.14",
